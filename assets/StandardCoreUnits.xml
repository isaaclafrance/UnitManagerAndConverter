<!-- TRIGONOMETRIC UNITS -->
	
	<unit>
	   	<unitName>hertz</unitName>
	   	<unitSystem>si</unitSystem>
	    <abbreviation>hz</abbreviation>
	   	<unitCategory>trigonometric_unit</unitCategory>
	    <componentUnits>
	        <component>
	            <unitName>second</unitName>
	            <exponent>-1.0f</exponent>
	        </component>
	    </componentUnits>
	    <baseConversionPolyCoeffs>
	    	<baseUnit>radian</baseUnit>
	        <polynomialCoeffs>6.283f 0.0f</polynomialCoeffs> 	                  
	    </baseConversionPolyCoeffs>
	</unit>  
	
	<unit>
	   	<unitName>degree</unitName>
