<!-- TRIGONOMETRIC UNITS -->
		
	<unit>
	   	<unitName>degree</unitName>
	   	<unitSystem>si</unitSystem>
	    <abbreviation>deg</abbreviation>
	    <unitCategory>trigonometric_unit</unitCategory>
	   	<unitDescription>A measurement of a plane angle, defined so that a full rotation is 360 degrees</unitDescription>
	    <componentUnits>
	        <component>
	            <unitName>degree</unitName>
	            <exponent>1.0f</exponent>
	        </component>
	    </componentUnits>
	    <baseConversionPolyCoeffs>
	    	<baseUnit>radian</baseUnit>
	        <polynomialCoeffs>0.017453f 0.0f</polynomialCoeffs> 	                
	    </baseConversionPolyCoeffs>
	</unit> 
			
	<unit>
	   	<unitName>radian</unitName>
	   	<unitSystem>si</unitSystem>
	    <abbreviation>rad</abbreviation>
	    <unitCategory>trigonometric_unit</unitCategory>	 
	    <unitDescription>Describes the plane angle subtended by a circular arc as the length of the arc divided by the radius of the arc</unitDescription>   
	    <componentUnits>
	        <component>
	            <unitName>radian</unitName>
	            <exponent>1.0f</exponent>
	        </component>
	    </componentUnits>
	    <baseConversionPolyCoeffs>
	    	<baseUnit>radian</baseUnit>
	        <polynomialCoeffs>1.0f 0.0f</polynomialCoeffs> 	                 
	    </baseConversionPolyCoeffs>
	</unit>    	

